' Preview durch <Alt>d

@startuml ER Diagramm

' hide the spot
hide circle

' evtl. Prbleme mit Kraehenfuessen vermeiden
skinparam linetype ortho

entity "Buch" {
  *id : UUID <<generated>>
  --
  *version: integer
  *titel : text
  *rating: number
  *art: text
  *verlag: text
  *preis: number
  *rabatt: number
  *lieferbar: boolean
  datum: date
  *isbn: text
  homepage: text
  *erzeugt: timestamp
  *aktualisiert: timestamp
}

entity "Schlagwort" {
  *id : UUID <<generated>>
  --
  *buch_id : UUID <<FK>>
  *schlagwort : text
}

Buch ||..o{ Schlagwort

footer (c) JÃ¼rgen Zimmermann

@enduml
